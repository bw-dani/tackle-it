ry: ''\n  })\n  const { id } = useParams();\n\n  useEffect(() => {\n    const prefillForm = () => {\n      const taskItem = props.tasks.find(task => task.id === Number(id));\n      setFormData({\n        image_url: taskItem.image_url,\n        description: taskItem.description,\n        deadline: taskItem.deadline,\n        category: taskItem.category\n      })\n    }\n    if (props.tasks.length) {\n      prefillForm();\n    }\n  }, [props.tasks])\n\n  const handleChange = (e) => {\n    const { name, value } = e.target;\n    setFormData(prevState => ({\n      ...prevState,\n      [name]: value\n    }))\n  }\n\n  return (\n    <div>\n\n      <div className='task-edit-pg'>\n        <form onSubmit={(e) => {\n          e.preventDefault();\n          props.handleUpdate(id, formData);\n        }}>\n          <h3>Edit Task: </h3>\n          <div className='input'>\n            <label className='input-label'>Image:\n              <input\n                className='input-text'\n                type='text'\n                name='image_url'\n                value={formData.image_url}\n                onChange={handleChange}\n              />\n            </label>\n            <br />\n            <label className='input-label'>Description:\n              <textarea\n                className='input-text-description'\n                type='text'\n                rows='5'\n                cols='40'\n                name='description'\n                value={formData.description}\n                onChange={handleChange}\n              ></textarea>\n            </label>\n            <br />\n            <label className='input-label'>Deadline:\n              <input\n                className='input-text'\n                type='text'\n                name='deadline'\n                value={formData.deadline}\n                onChange={handleChange}\n              />\n            </label>\n            <br />\n            <label className='input-label'>Category:\n              <input\n 