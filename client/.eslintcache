tAllTasks} from './screens/tasks/Tasks'\nimport Homepage from './screens/homepage/Homepage'\nimport { loginUser, registerUser, removeToken, verifyUser } from './services/auth';\nimport TaskEdit from './screens/edit-task/TaskEdit';\nimport TaskCreate from './screens/create-task/TaskCreate'\nimport { destroyTask, getAllTask, postTask, putTask } from './services/tasks'\nimport TaskDetail from './screens/task-detail/TaskDetail';\nimport Settings from './screens/settings/Settings';\n\n\nfunction App() {\n  const [currentUser, setCurrentUser] = useState(null);\n  const [tasks, setTasks] = useState([]);\n\n  const history = useHistory()\n\n  useEffect(() => {\n    const handleVerify = async () => {\n      const userData = await verifyUser();\n      setCurrentUser(userData);\n      if (!userData) {\n        history.push('/')\n      }\n    }\n    handleVerify();\n  }, [])\n\n  const handleLogin = async (loginData) => {\n    const userData = await loginUser(loginData);\n    setCurrentUser(userData);\n    history.push('/homepage');\n  }\n\n  const handleSignup = async (registerData) => {\n    const userData = await registerUser(registerData);\n    setCurrentUser(userData);\n    history.push('/homepage');\n  }\n\n  const handleLogout = () => {\n    setCurrentUser(null);\n    localStorage.removeItem('authToken');\n    removeToken();\n    history.push('/');\n  }\n\n  const handleCreate = async (taskData) => {\n    const newTask = await postTask(taskData);\n    setTasks(prevState => [...prevState, newTask]);\n    history.push('/homepage');\n  }\n\n  const handleUpdate = async (id, taskData) => {\n    const updatedTask = await putTask(id, taskData);\n    setTasks(prevState => prevState.map(task => {\n      return task.id === Number(id) ? updatedTask : task\n    }))\n    history.push('/homepage');\n  }\n\n  const handleDelete = async (id) => {\n    await destroyTask(id);\n    setTasks(prevState => prevState.filter(task => task.id !== id))\n    history.push('/homepage');\n  }\n\n\n  return (\n\n <Switch>\n        <Route exact path='/'>\n      {/* login */}\n    \n      <Login handleLogin={handleLogin} />\n     \n      </Route>\n      <Route path='/signup'>\n          {/* signup */}\n          <SignUp handleSignup={handleSignup} />\n      </Route>\n\n      <Route path='/homepage'>\n          {/* homepage */}\n        <Homepage currentUser={currentUser}/>\n      </Route>\n      \n      <Route path='/tasks/new'>\n        <TaskCreate handleCreate={handleCreate} />\n      </Route>\n\n        <Route path='/tasks/:id/edit'>\n          {/* homepage */}\n        <TaskEdit tasks={tasks} handleUpdate={handleUpdate}/>\n        </Route>\n        \n        <Route path='/task-detail/:id'>\n          {/* edit task */}\n        <TaskDetail currentUser={currentUser} handleDelete={handleDelete} />\n      </Route>\n      \n      <Route path='/settings'>\n        <Settings currentUser={currentUser}  />\n        </Route>\n</Switch>\n      \n  );\n}\n\nexport default App;\n\n","/Users/danibw/General_Assembly/unit_4/P4/tackle-it/client/src/reportWebVitals.js",[],"/Users/danibw/General_Assembly/unit_4/P4/tackle-it/client/src/layouts/Layout.jsx",["113"],"import Header from \"../components/Header\";\nimport Sidebar from \"../components/sidebar/Sidebar\";\n\n\nexport default function Layout(props) {\n  return (\n    <div>\n      <Header\n       currentUser={props.currentUser}\n        handleLogout={props.handleLogout}\n      />\n     {props.children}\n    </div>\n  )\n}\n","/Users/danibw/General_Assembly/unit_4/P4/tackle-it/client/src/components/sidebar/Sidebar.jsx",["114","115","116"],"import React from 'react';\nimport { Link } from 'react-router-dom';\nimport styled from 'styled-components'\nimport Profile from './Profile';\nimport Menu from './menu/Menu';\nimport ToggleSwitch from './ToggleSwitch'\n\nconst Container = styled.div`\n    background-color: ${({ theme }) => theme.secondary};\n    position: fixed;\n    left: 0;\n    top: 0;\n    bottom: 0;\n    width: 16rem;\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    `\n\nexport default function Sidebar(props) {\n  const { currentUser, handleLogout } = props;\n  return (\n    <Container>\n            <Profile />\n            <Menu />\n            <ToggleSwitch />\n        </Container>\n  )\n}","/Users/danibw/General_Assembly/unit_4/P4/tackle-it/client/src/components/sidebar/ToggleSwitch.jsx",[],"/Users/danibw/General_Assembly/unit_4/P4/tackle-it/client/src/components/sidebar/Profile.jsx",["117","118"],"import React from 'react'\nimport styled from 'styled-components'\n// import Image from '../../assets/images/profilelg.png'\n\nconst Container = styled.div`\n    margin-top: 5rem;\n`\n\nconst ProfileImg = styled.img`\n    height: 5rem;\n`\nconst ProfileName = styled.h1`\n    font-size: 1rem;\n    font-weight: 300;\n    color: ${({ theme }) => theme.textColor};\n`\n\nconst Profile = (props) => {\n  const { currentUser} = props;\n    return (\n        <Container>\n        {/* <ProfileImg src={Image} /> */}\n        <div>\n          {\n            currentUser ?\n              <>\n                <p>{currentUser?.name}</p>\n              </>\n              :\n              <p>{currentUser?.username}</p>\n        }\n            \n           \n        \n        </div>\n        </Container>\n    )\n}\n\nexport default Profile","/Users/danibw/General_Assembly/unit_4/P4/tackle-it/client/src/components/sidebar/menu/Menu.jsx",[],"/Users/danibw/General_Assembly/unit_4/P4/tackle-it/client/src/components/sidebar/menu/MenuLink.jsx",[],"/Users/danibw/General_Assembly/unit_4/P4/tackle-it/client/src/assets/ThemeSwitch.jsx",[],"/Users/danibw/General_Assembly/unit_4/P4/tackle-it/client/src/screens/login/Login.jsx",[],"/Users/danibw/General_Assembly/unit_4/P4/tackle-it/client/src/services/auth.js",[],"/Users/danibw/General_Assembly/unit_4/P4/tackle-it/client/src/services/apiConfig.js",[],"/Users/danibw/General_Assembly/unit_4/P4/tackle-it/client/src/components/Header.jsx