gory\n        <input\n          type='text'\n          name='category'\n          value={formData.category}\n          onChange={handleChange}\n        />\n      </label>\n      <br/>\n      <button>Submit</button>\n    </form>\n  )\n}","/Users/danibw/General_Assembly/unit_4/P4/tackle-it/client/src/screens/create-task/TaskCreate.jsx",[],"/Users/danibw/General_Assembly/unit_4/P4/tackle-it/client/src/screens/settings/Settings.jsx",[],{"ruleId":"125","replacedBy":"126"},{"ruleId":"127","replacedBy":"128"},{"ruleId":"129","severity":1,"message":"130","line":7,"column":8,"nodeType":"131","messageId":"132","endLine":7,"endColumn":14},{"ruleId":"129","severity":1,"message":"133","line":11,"column":9,"nodeType":"131","messageId":"132","endLine":11,"endColumn":20},{"ruleId":"129","severity":1,"message":"134","line":16,"column":23,"nodeType":"131","messageId":"132","endLine":16,"endColumn":33},{"ruleId":"135","severity":1,"message":"136","line":36,"column":6,"nodeType":"137","endLine":36,"endColumn":8,"suggestions":"138"},{"ruleId":"129","severity":1,"message":"139","line":50,"column":9,"nodeType":"131","messageId":"132","endLine":50,"endColumn":21},{"ruleId":"129","severity":1,"message":"140","line":2,"column":8,"nodeType":"131","messageId":"132","endLine":2,"endColumn":15},{"ruleId":"129","severity":1,"message":"141","line":2,"column":10,"nodeType":"131","messageId":"132","endLine":2,"endColumn":14},{"ruleId":"129","severity":1,"message":"142","line":21,"column":11,"nodeType":"131","messageId":"132","endLine":21,"endColumn":22},{"ruleId":"129","severity":1,"message":"139","line":21,"column":24,"nodeType":"131","messageId":"132","endLine":21,"endColumn":36},{"ruleId":"129","severity":1,"message":"143","line":9,"column":7,"nodeType":"131","messageId":"132","endLine":9,"endColumn":17},{"ruleId":"129","severity":1,"message":"144","line":12,"column":7,"nodeType":"131","messageId":"132","endLine":12,"endColumn":18},{"ruleId":"129","severity":1,"message":"145","line":2,"column":10